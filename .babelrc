{
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "chrome": "67"
          },
          "useBuiltIns": "usage",
          "corejs": 2
        }
      ],
      "@babel/preset-react"
    ],
      "plugins": [
        [
          "@babel/plugin-transform-runtime",
          {
            "absoluteRuntime": false,
            "helpers": true,
            "regenerator": true,
            "useESModules": false
          }
        ]
      ]
  }